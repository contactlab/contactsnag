{
  "name": "contactsnag",
  "version": "2.1.0",
  "description": "Bugsnag utilities for Contactlab's applications",
  "main": "lib/index.js",
  "module": "lib/es6/index.js",
  "typings": "lib/index.d.ts",
  "sideEffects": false,
  "author": "Contactlab",
  "license": "Apache-2.0",
  "repository": "contactlab/contactsnag",
  "bugs": {
    "url": "https://github.com/contactlab/contactsnag/issues"
  },
  "homepage": "https://github.com/contactlab/contactsnag#readme",
  "engines": {
    "node": ">=8.10 <11.0",
    "npm": ">=6.0"
  },
  "files": [
    "lib",
    "typings"
  ],
  "bin": "./lib/bin/cli.js",
  "keywords": [
    "contactlab",
    "clab",
    "bugsnag",
    "wrapper"
  ],
  "scripts": {
    "check": "tsc --noEmit",
    "lint": "tslint -p .",
    "jest": "jest --ci --coverage",
    "pree2e": "npm run build:bin",
    "e2e": "ts-node test/e2e/",
    "pretest": "npm run check && npm run lint",
    "test": "npm run jest && npm run e2e",
    "prebuild": "rimraf ./lib",
    "build": "npm run build:std && npm run build:es6 && npm run build:bin",
    "build:std": "tsc -p ./tsconfig.build.json",
    "build:es6": "tsc -p ./tsconfig.build-es6.json",
    "build:bin": "tsc -p ./tsconfig.build-bin.json",
    "prepublishOnly": "npm test && npm run build"
  },
  "dependencies": {
    "@bugsnag/js": "6.3.2",
    "bugsnag-sourcemaps": "1.2.1",
    "chalk": "2.4.2",
    "fp-ts": "^1.19.0",
    "io-ts": "^1.8.5",
    "read-pkg-up": "6.0.0",
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "@contactlab/tslint-config": "1.3.0",
    "@types/jest": "24.0.15",
    "@types/node": "12.6.1",
    "husky": "3.0.0",
    "jest": "24.8.0",
    "mitm": "1.7.0",
    "prettier": "1.18.2",
    "pretty-quick": "1.11.1",
    "rimraf": "2.6.3",
    "ts-jest": "24.0.2",
    "ts-node": "8.3.0",
    "tslint": "5.18.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
