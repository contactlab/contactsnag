{
  "name": "contactsnag",
  "version": "2.0.2",
  "description": "Bugsnag utilities for Contactlab's applications",
  "main": "lib/index.js",
  "module": "lib/es6/index.js",
  "typings": "lib/index.d.ts",
  "sideEffects": false,
  "author": "Contactlab",
  "license": "Apache-2.0",
  "repository": "contactlab/contactsnag",
  "bugs": {
    "url": "https://github.com/contactlab/contactsnag/issues"
  },
  "homepage": "https://github.com/contactlab/contactsnag#readme",
  "engines": {
    "node": ">=8.10 <11.0",
    "npm": ">=6.0"
  },
  "files": [
    "lib",
    "typings"
  ],
  "bin": {
    "contactsnag": "./lib/bin/cli.js"
  },
  "keywords": [
    "contactlab",
    "clab",
    "bugsnag",
    "wrapper"
  ],
  "scripts": {
    "check": "tsc --noEmit",
    "lint": "tslint -p .",
    "jest": "jest --ci --coverage --config jest.config.json",
    "pretest": "npm run check && npm run lint",
    "test": "npm run jest",
    "prebuild": "rimraf ./lib",
    "build": "npm run build:std && npm run build:es6 && npm run build:bin",
    "build:std": "tsc -p ./tsconfig.build.json",
    "build:es6": "tsc -p ./tsconfig.build-es6.json",
    "build:bin": "tsc -p ./tsconfig.build-bin.json",
    "prepublishOnly": "npm test && npm run build"
  },
  "dependencies": {
    "@bugsnag/js": "6.2.0",
    "bugsnag-sourcemaps": "1.1.0",
    "chalk": "2.4.2",
    "fp-ts": "^1.14.3",
    "io-ts": "^1.8.5",
    "read-pkg-up": "5.0.0",
    "tslib": "1.9.3"
  },
  "devDependencies": {
    "@contactlab/tslint-config": "1.3.0",
    "@types/jest": "24.0.11",
    "@types/node": "11.13.7",
    "@types/read-pkg-up": "^3.0.1",
    "husky": "2.1.0",
    "jest": "24.7.1",
    "prettier": "1.17.0",
    "pretty-quick": "1.10.0",
    "rimraf": "2.6.3",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "tslint": "5.15.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.4.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
