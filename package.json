{
  "name": "contactsnag",
  "version": "7.0.0",
  "description": "Bugsnag utilities for Contactlab's applications",
  "main": "lib/index.js",
  "module": "lib/es6/index.js",
  "typings": "lib/index.d.ts",
  "sideEffects": false,
  "author": "Contactlab",
  "license": "Apache-2.0",
  "repository": "contactlab/contactsnag",
  "bugs": {
    "url": "https://github.com/contactlab/contactsnag/issues"
  },
  "homepage": "https://contactlab.github.io/contactsnag",
  "engines": {
    "node": ">=12.0",
    "npm": ">=6.0"
  },
  "files": [
    "lib",
    "typings"
  ],
  "bin": "./lib/bin/cli.js",
  "keywords": [
    "contactlab",
    "bugsnag",
    "wrapper"
  ],
  "scripts": {
    "postinstall": "husky install",
    "check": "tsc --noEmit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "pretest": "npm run check && npm run lint",
    "test": "jest",
    "prebuild": "rimraf ./lib",
    "build": "npm run build:std && npm run build:es6 && npm run build:bin",
    "build:std": "tsc -p ./tsconfig.build.json",
    "build:es6": "tsc -p ./tsconfig.build-es6.json",
    "build:bin": "tsc -p ./tsconfig.build-bin.json",
    "prepublishOnly": "npm test && npm run build"
  },
  "peerDependencies": {
    "fp-ts": "^2.8.0",
    "io-ts": "^2.2.10"
  },
  "dependencies": {
    "@bugsnag/js": "7.5.4",
    "@types/webpack-bugsnag-plugins": "^1.4.0",
    "bugsnag-sourcemaps": "1.3.0",
    "chalk": "4.1.0",
    "read-pkg-up": "7.0.1",
    "tslib": "^2.0.0",
    "webpack-bugsnag-plugins": "^1.6.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^12.12.25",
    "eslint": "^7.17.0",
    "eslint-config-contactlab": "^6.1.0",
    "eslint-config-prettier": "^7.1.0",
    "fp-ts": "^2.8.0",
    "globalthis": "^1.0.1",
    "husky": "^5.0.6",
    "io-ts": "^2.2.10",
    "jest": "^26.6.3",
    "pinst": "^2.1.1",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.0.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "jest": {
    "automock": false,
    "bail": true,
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "<rootDir>/test/bin/_data.ts",
      "<rootDir>/node_modules/"
    ],
    "coverageReporters": [
      "text"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": true
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "node",
      "ts"
    ],
    "preset": "ts-jest",
    "roots": [
      "<rootDir>/test/"
    ],
    "testMatch": null,
    "testRegex": "(\\.|/)spec\\.ts$"
  }
}
