{
  "name": "contactsnag",
  "version": "1.2.1",
  "description": "Bugsnag utilities for Contactlab's applications",
  "main": "dist.js",
  "engines": {
    "node": ">= 8.0",
    "npm": ">= 5.2"
  },
  "scripts": {
    "build": "babel index.js -o dist.js",
    "presetup": "npm run build",
    "setup": "node ./scripts/prepare-test",
    "lint": "eslint index.js ./bin",
    "pretest": "npm run setup && npm run lint",
    "test": "nyc ava"
  },
  "bin": {
    "contactsnag": "./bin/upload.js"
  },
  "files": [
    "bin",
    "dist.js"
  ],
  "repository": "contactlab/contactsnag",
  "keywords": [
    "bugsnag",
    "node",
    "javascript",
    "wrapper",
    "cli",
    "utility",
    "contactlab"
  ],
  "author": "Contactlab Front-end Team",
  "contributors": [
    "Salvatore Laisa <salvatore.laisa@contactlab.it>",
    "Stefano Magrassi <stefano.magrassi@gmail.com>",
    "Laura Santalucia"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/contactlab/contactsnag/issues"
  },
  "homepage": "https://github.com/contactlab/contactsnag#readme",
  "dependencies": {
    "bugsnag-js": "3.3.1",
    "bugsnag-sourcemaps": "1.0.1",
    "find-root": "1.1.0"
  },
  "devDependencies": {
    "@contactlab/babelrc": "1.0.0",
    "ava": "0.23.0",
    "babel-cli": "6.26.0",
    "babel-register": "6.26.0",
    "eslint": "4.10.0",
    "eslint-config-contactlab": "1.0.0",
    "mock-browser": "0.92.14",
    "nyc": "11.4.1",
    "proxyquire": "1.8.0",
    "sinon": "4.0.2"
  },
  "ava": {
    "files": [
      "./test/**/*.test.js"
    ],
    "failFast": true,
    "failWithoutAssertions": false,
    "require": [
      "babel-register"
    ]
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "extends": [
      "contactlab"
    ],
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    }
  }
}
