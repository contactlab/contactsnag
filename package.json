{
  "name": "contactsnag",
  "version": "1.3.1",
  "description": "Bugsnag utilities for Contactlab's applications",
  "main": "lib/index.js",
  "module": "lib/es6/index.js",
  "typings": "lib/index.d.ts",
  "sideEffects": false,
  "author": "Contactlab",
  "license": "Apache-2.0",
  "repository": "contactlab/contactsnag",
  "bugs": {
    "url": "https://github.com/contactlab/contactsnag/issues"
  },
  "homepage": "https://github.com/contactlab/contactsnag#readme",
  "engines": {
    "node": ">= 8.10",
    "npm": ">= 6.0"
  },
  "files": [
    "lib"
  ],
  "bin": {
    "contactsnag": "./lib/bin/upload.js"
  },
  "keywords": [
    "contactlab",
    "clab",
    "bugsnag",
    "wrapper"
  ],
  "scripts": {
    "check": "tsc --noEmit",
    "build": "babel index.js -o dist.js",
    "presetup": "npm run build",
    "setup": "node ./scripts/prepare-test",
    "lint": "eslint index.js ./bin",
    "pretest": "npm run setup && npm run lint",
    "test": "ava"
  },
  "dependencies": {
    "bugsnag-js": "3.3.1",
    "bugsnag-sourcemaps": "1.0.1",
    "find-root": "1.1.0"
  },
  "devDependencies": {
    "@contactlab/babelrc": "1.0.0",
    "ava": "0.23.0",
    "eslint": "4.10.0",
    "eslint-config-contactlab": "1.0.0",
    "mock-browser": "0.92.14",
    "proxyquire": "1.8.0",
    "sinon": "4.0.2",
    "typescript": "3.3.3333"
  },
  "ava": {
    "files": [
      "./test/**/*.test.js"
    ],
    "failFast": true,
    "failWithoutAssertions": false,
    "require": [
      "babel-register"
    ]
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "extends": [
      "contactlab"
    ],
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    }
  }
}
